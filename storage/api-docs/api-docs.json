        "/telephony/api/cucm/site": {
            "post": {
                "tags": [
                    "Management - CUCM - Site Provisioning"
                ],
                "summary": "Create New Site in CUCM",
                "description": "\n     Select the correct Site Design Type according to your site's specific requirements.\n     Type 1 - Centralized SIP Trunking and Centralized E911\n     Type 2 - Local Gateway Trunking but using Centralized E911\n     Type 3 - Centralized SIP Trunking but leveraging local gateway/SRST for 911\n     Type 4 - Local Gateway Trunking and 911",
                "operationId": "createSite",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "sitecode",
                        "in": "formData",
                        "description": "Sitecode",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "type",
                        "in": "formData",
                        "description": "Design Type - See Implementation Notes Above",
                        "required": true,
                        "type": "integer",
                        "enum": [
                            "1",
                            "2",
                            "3",
                            "4"
                        ]
                    },
                    {
                        "name": "srstip",
                        "in": "formData",
                        "description": "SRST IP Address - Not required for Type 1 Designs. Recommended but not required for Type 2,3, and 4",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "h323ip",
                        "in": "formData",
                        "description": "These are required for Design Type 2,3,and 4. If multiple H323 Gateways, enter each one on new line. These will get added to the site route group. ",
                        "required": false,
                        "type": "array",
                        "items": {
                            "description": "H323 Gateways",
                            "type": "string"
                        }
                    },
                    {
                        "name": "timezone",
                        "in": "formData",
                        "description": "TimeZone and Format - These are prebuilt in CUCM and may need customized to your environment. ",
                        "required": true,
                        "type": "string",
                        "enum": [
                            "Alaska-12",
                            "Arizona-12",
                            "Central-12",
                            "Eastern-12",
                            "Hawaii-12",
                            "Mountain-12",
                            "Pacific-12"
                        ]
                    },
                    {
                        "name": "npa",
                        "in": "formData",
                        "description": "NPA (###) - Area Code",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "nxx",
                        "in": "formData",
                        "description": "NXX (###) - Prefix",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "didrange",
                        "in": "formData",
                        "description": "Last 4 digit Ranges. Use Regex to represent the DID Ranges. Use multiple Lines to represent multiple ranges. Example: 40[2-9]X",
                        "required": true,
                        "type": "array",
                        "items": {
                            "description": "",
                            "type": "string"
                        }
                    },
                    {
                        "name": "operator",
                        "in": "formData",
                        "description": "Operator Last 4 digits of DID",
                        "required": false,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "401": {
                        "description": "Unauthorized user"
                    }
                }
            }
        },
